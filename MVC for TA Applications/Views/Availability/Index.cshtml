@model IEnumerable<MVC_for_TA_Applications.Models.Availability>

@{
    ViewData["Title"] = "Index";
}
<head>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pixi.js/5.1.3/pixi.min.js"></script>
    
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>


</head>


<h1 class="header">Schedule Builder</h1>

<body>
    <div id="canvas_div"></div>
    <script type="text/javascript" src="~/js/schedule.js"></script>

    @{

        foreach (var item in Model)
        {

            if (item.Available == true)
            {
                <script type="text/javascript">fillIn('@item.AvailabilityString');</script>
            }
        }
    }
    <a id="myBtn" class="btn btn-danger btn-lg" onclick="call_Set_Schedule()"> submit </a>
    <p id= "save_data_indicator"></p>
</body>



